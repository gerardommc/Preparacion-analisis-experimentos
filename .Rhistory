levels(conc.f) <- c("Baja", "Baja", "Media", "Media", "Alta", "Alta", "Alta")
df.1$conc.factor <- conc.f
df.1 <- df.1[df.1$conc< 1000, ]
df.1 <- subset(df.1, select = c("uptake", "conc.factor", "Treatment", "Type"))
names(df.1) <- c("Captura", "Concentración", "Tratamiento", "Ciudad")
ggplot(df.1) + geom_violin(aes(x = Concentración, y = Captura, colour = Tratamiento, fill = Tratamiento), alpha = 0.5)
ggplot(df.1) + geom_boxplot(aes(x = Concentración, y = Captura, colour = Tratamiento, fill = Tratamiento), alpha = 0.5)
m1 <- lme4::lmer(Captura ~ Concentración * Tratamiento + (1|Ciudad), df.1)
m1
car::Anova(m1, type = 3)
plot( m1)
car::Anova(m1, type = 2)
write.csv(df.1, "Unidad 3-ANOVA II/CO2-2.csv")
